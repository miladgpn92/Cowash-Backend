@using SharedModels.Dtos
@model SettingSelectDto

 
<!-- Video -->
<section class="video-wrapper video section-padding bg-img bg-fixed" data-background="/Template/img/slider/1.png"
         data-overlay-dark="5">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <div class="section-subtitle">
                    <vc:content id="10"></vc:content>
                </div>
                <div class="section-title white">
                    <vc:content id="11"></vc:content>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="text-center col-md-12">
                <a class="vid" href="@Model.IntroVideoUrl?.Trim('[', ']', '"')">
                    <div class="vid-butn">
                        <span class="icon">
                            <i class="ti-control-play">
                            </i>
                        </span>
                    </div>
                </a>
            </div>
        </div>
    </div>
</section>