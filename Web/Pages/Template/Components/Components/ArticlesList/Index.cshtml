@using SharedModels.Dtos;
@model List<ArticleSelectDto>

 



<!-- Our Blog Section Start -->
<div class="our-blog">
    <div class="container">
        <div class="row section-row">
            <div class="col-lg-12">
                <!-- Section Title Start -->
                <div class="section-title">
                    <vc:content id="19" tag="h3" class="wow fadeInUp"></vc:content>
        
                    <h2 class="" data-cursor="-opaque"> <span><vc:content id="20"></vc:content></span>  <vc:content id="21"></vc:content> </h2>
                </div>
                <!-- Section Title End -->
            </div>
        </div>

        <div class="row">

            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-md-6">
                    <!-- Post Item Start -->
                    <vc:article-cart model="item" class=""></vc:article-cart>
                    <!-- Post Item End -->
                </div>
           


            }

        
 
        </div>
    </div>
</div>
<!-- Our Blog Section End -->

